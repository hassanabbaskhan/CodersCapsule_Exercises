/*
 24. Write a program to display the first 10 odd numbers using do while loops.
 
 */


using System;
namespace Question_14_oddNumber
{
    internal class Program
    {
        static void Main(string[] args)
        {

            int x = 1;

            do
            {
                Console.WriteLine(x);
                x = x + 2;
            }

            while (x < 20);
 
            Console.ReadLine();

        }
        
    }
}

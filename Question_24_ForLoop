/*
 24. Write a program to display the first 10 odd numbers using for loops.
 
 */


using System;
namespace Question_14_oddNumber
{
    internal class Program
    {
        static void Main(string[] args)
        {



        for (int x = 1; x< 20; x=x+2)
            {
                Console.WriteLine(x);
            }
 
            Console.ReadLine();

        }
        
    }
}
